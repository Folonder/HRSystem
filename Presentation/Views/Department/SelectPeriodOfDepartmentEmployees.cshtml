@model Presentation.Models.Department.PeriodOfDepartmentEmployeesModel

<form asp-controller="Department" asp-action="EmployeePeriod" method="post">
    <div class="form-group">
        <label asp-for="StartDate">Начальная дата:</label>
        <input asp-for="StartDate" type="date" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="EndDate">Конечная дата:</label>
        <input asp-for="EndDate" type="date" class="form-control" />
    </div>
    <p></p>
    <div class="form-group">
        <input type="hidden" asp-for="DepartmentId" />
            <button type="submit" class="btn btn-primary">Показать сотрудников</button>
        <a asp-action="Index" class="btn btn-secondary">Назад к списку</a>
    </div>
</form>